{
  "name": "SJ utan sol",
  "version": "1.0.0",
  "description":
    "An extension to avoid sitting in the sunlight when riding SJ trains",
  "author": "Sebastian Andr√©asson",
  "manifest_version": 2,
  "permissions": ["*://*.sj.se/*", "activeTab"],
  "content_scripts": [
    {
      "js": ["js/inject.js"],
      "run_at": "document_end",
      "matches": ["*://*.sj.se/*"],
      "all_frames": true
    }
  ],
  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
